{
  "name": "website-starter",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "clean:dist": "rimraf dist",
    "dev": "webpack-dev-server --inline --devtool eval --progress --colors --port 3000 --content-base src",
    "prebuild": "npm run clean:dist",
    "build:prod": "webpack -p --config webpack-prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "build": "webpack -p",
    "lint": "eslint src/ ; true",
    "postinstall": "webpack --config ./webpack-prod.config.js --progress --colors",
    "start": "node server.js",
    "server:prod": "http-server dist --cors",
    "deploy": "ssh kitconcept@kitconcept.com 'cd /srv/new.kitconcept.com; git checkout -f kitconcept.com; git pull; npm install; npm run build:prod'",
    "psi": "psi new.kitconcept.com --threshold 99 ; true",
    "webpagetest": "webpagetest test http://new.kitconcept.com  -k $WEBPAGETEST_API_KEY --poll --specs testspecs.json --reporter spec ; true"
  },
  "engines": {
    "node": "4.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tisto/website-starter.git"
  },
  "author": "Timo Stollenwerk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tisto/website-starter/issues"
  },
  "homepage": "https://github.com/tisto/website-starter#readme",
  "devDependencies": {
    "babel-core": "^5.8.33",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.23.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "image-webpack-loader": "^1.6.2",
    "imagemin-jpegtran": "^4.3.2",
    "node-sass": "^3.4.2",
    "purifycss-webpack-plugin": "^2.0.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.8",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.5",
    "express": "^4.13.3",
    "jquery": "^2.1.4",
    "jquery-placeholder": "^2.3.1",
    "materialize-css": "^0.97.5",
    "psi": "^2.0.2",
    "swiper": "^3.3.0"
  }
}
